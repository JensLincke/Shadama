breed Turtle (pos:vec2, new:float)
patch Patch (pos:vec2)

def foo(patch:object, vec:vec2) {
    patch.pos = this.pos:vec2
}

def bar(patch:object) {
  this.pos = patch.pos:vec2
  var v = patch.pos;
  this.new = v.x;
}

static x() {
  Turtle.fillSpace("pos", vec2(100, 100))
}

static y() {
  Turtle.foo(Patch, vec2(100, 100) + vec2(50, 50))
}

static z() {
  Turtle.bar(Patch)
}
