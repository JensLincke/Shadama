program "Fill"
breed Turtle (x, y, r, g, b, a)

def setColor() {
  this.r = this.x / 512;
  this.g = this.y / 512;
  this.b = 0.0;
  this.a = 1.0;
}

static setup() {
  Turtle.setCount(3000);
  Turtle.fillRandom("x", 0, 512);
  Turtle.fillRandom("y", 0, 512);
  Turtle.setColor();
  Turtle.draw();
}

def move() {
  this.x = this.x + 1;
  this.y = this.y + 1;
}

static loop() {
  Turtle.move();
  Turtle.draw();
}
